    "npcyell_ids_10714_Text": "Verdammt!",
    "npcyell_ids_12159_Text": "Eine Falle!",
    "npcyell_ids_12226_Text": "Verdammt!",
    "npcyell_ids_13202_Text": "Verdammt!",
    "npcyell_ids_14972_Text": "Verdammt!",
    "npcyell_ids_16686_Text": "Verdammt!",
    "npcyell_ids_16840_Text": "Verdammt!",
    "npcyell_ids_16845_Text": "Verdammt!",
    "npcyell_ids_2141_Text": "Verdammt!",
    "npcyell_ids_2361_Text": "Verdammt!",
    "npcyell_ids_3016_Text": "Eine Falle!",
    "npcyell_ids_3756_Text": "Verdammt!",
    "npcyell_ids_4947_Text": "Verdammt!",
    "npcyell_ids_5785_Text": "Verdammt!",
    "npcyell_ids_6585_Text": "Verdammt!",
    "npcyell_ids_8177_Text": "Schreie? Und was ist das für ein Gestank?",
    "npcyell_ids_8178_Text": "Anemonen! Stinkende Anemonen!",
    "npcyell_ids_8179_Text": "Zieh die Waffe und halt die Luft an!",
    "npcyell_ids_8180_Text": "Ich halt den Mief nicht aus!",
    "npcyell_ids_8181_Text": "Mir kommt's hoch...",
    "npcyell_ids_8182_Text": "Meine armen Fische...",
    "npcyell_ids_8183_Text": "Der Gestank ist fort! Oder meine Nase hat aufgegeben.",
    "npcyell_ids_8184_Text": "Moment, was ist das?",
    "npcyell_ids_8185_Text": "Uaaah! Was für eine Bestie!",
    "npcyell_ids_8186_Text": "Der hat die Anemonen wohl auch gerochen.",
    "npcyell_ids_8187_Text": "Und glücklich guckt er nicht drein!",
    "npcyell_ids_8188_Text": "<keuch> Schnell weg, bevor hier noch was aufkreuzt.",
    "npcyell_ids_8256_Text": "Den Heldenmut werden wir euch austreiben!",
    "npcyell_ids_8280_Text": "So leicht kommt ihr nicht davon!",
    "npcyell_ids_8378_Text": "Aber merkt euch eins. Ihr seid nirgendwo sicher.",
    "npcyell_ids_8379_Text": "Und noch etwas: Das Seenland gehört uns! Eines Tages!",
    "npcyell_ids_8420_Text": "Ich blute! Und nicht zu knapp!",
    "npcyell_ids_8421_Text": "<seufz> Warte. Ich drück was drauf.",
    "npcyell_ids_8426_Text": "Das war der letzte von ihnen.",
    "npcyell_ids_8427_Text": "Keine Sorge. Die Wunde wird heilen.",
    "npcyell_ids_8428_Text": "Wir ziehen dann mal weiter. Wo ist noch mal der Korkenzieher?",
    "npcyell_ids_8429_Text": "Nein, keine spitzen Gegenstände mehr!",
    "npcyell_ids_8441_Text": "Verdammt!",
    "npcyell_ids_8523_Text": "Was ist hier geschehen? Wurdest du angegriffen?",
    "npcyell_ids_8524_Text": "Wer hat das getan?",
    "npcyell_ids_8527_Text": "Verdammt!",
    "npcyell_ids_8528_Text": "Eine Falle!",
    "npcyell_ids_8529_Text": "Sie sind überall!"
}